public class Summation{
    //Summation code --------------------------------------------------
    
    System.out.println("Please enter quantity of numbers");
    int qn= sc.nextInt();
    int sum=0, i;
    
    for(i=1; i<=qn; i++){
        System.out.println("Please enter your number");
        int num = sc.nextInt();
        sum= sum+ num;
    }
    return sum;
}
    