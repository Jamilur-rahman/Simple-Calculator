import java.util.Scanner;
public class Algebra{
    Scanner sc = new Scanner(System.in);
    System.out.println("Please enter a number");
    double num= sc.nextDouble;
    public void squre(){
        System.out.printf("Result = %.3f", Math.pow(num,2));
    }
    public void sqrt(){
         System.out.printf("Result = %.3f", Math.sqrt(num));
    }
    public void cube(){
         System.out.printf("Result = %.3f", Math.pow(num,3));
    }
    public void cubert(){
         System.out.print("Result = %.3f", Math.pow(num,(1/3)));
    }
}